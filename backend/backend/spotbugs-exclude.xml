<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <!-- Exclude entities from analysis (often have many false positives) -->
    <Match>
        <Package name="ma.nassnewsapp.backend.entities" />
    </Match>
    
    <!-- Exclude generated code -->
    <Match>
        <Source name="~.*\.class" />
    </Match>
    
    <!-- Exclude test classes from certain bug patterns -->
    <Match>
        <Class name="~.*Test" />
        <Bug pattern="DM_EXIT" />
    </Match>
    
    <!-- Exclude startup classes -->
    <Match>
        <Package name="ma.nassnewsapp.backend.startup" />
    </Match>
</FindBugsFilter>

